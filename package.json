{
	"devDependencies": {
		"@biomejs/biome": "2.3.11",
		"@changesets/cli": "^2.29.8",
		"@commitlint/cli": "^20.3.0",
		"@commitlint/config-conventional": "^20.3.0",
		"@commitlint/types": "^20.2.0",
		"@nx/js": "^22.3.3",
		"@nx/next": "^22.3.3",
		"husky": "^9.1.7",
		"knip": "^5.79.0",
		"nx": "^22.3.3"
	},
	"name": "jsandy",
	"packageManager": "bun@1.3.4",
	"private": true,
	"scripts": {
		"build": "nx run-many -t build",
		"build:packages": "nx run-many -t build --projects='packages/*'",
		"build:www": "nx run www:build:cf",
		"clean": "nx run-many -t clean",
		"deploy": "nx run-many -t deploy",
		"deploy:www": "nx run www:deploy",
		"dev": "nx run-many -t dev",
		"dev:www": "nx run www:dev",
		"format": "biome check --linter-enabled=false --write .",
		"lint": "biome lint --write .",
		"prepare": "husky",
		"release": "bun run build && changeset publish",
		"test": "nx run-many -t test",
		"test:coverage": "nx run-many -t test:coverage",
		"test:watch": "bun run --filter=@jsandy/rpc test:watch",
		"typecheck": "nx run-many -t typecheck"
	},
	"workspaces": [
		"apps/*",
		"packages/*"
	]
}
