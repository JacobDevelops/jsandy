{
	"compilerOptions": {
		"allowSyntheticDefaultImports": true,
		"jsx": "react-jsx",
		"jsxImportSource": "react",
		"lib": ["ES2019", "DOM", "DOM.Iterable", "ES2015.Iterable"],
		"module": "ES2022",
		"moduleResolution": "node",
		"noEmit": true,
		"outDir": "./dist",
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"strict": true,
		"target": "ESNext"
	}
}
