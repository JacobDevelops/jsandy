			"require": {
				"default": "./dist/index.js",
				"types": "./dist/index.d.ts"
			}
		},
		"./adapters": {
			"import": {
				"default": "./dist/sockets/adapters/index.mjs",
				"types": "./dist/sockets/adapters/index.d.mts"
			},
			"require": {
				"default": "./dist/sockets/adapters/index.js",
				"types": "./dist/sockets/adapters/index.d.ts"
			}
		},
		"./client": {
			"import": {
				"default": "./dist/rpc-client/index.mjs",
				"types": "./dist/rpc-client/index.d.mts"
			},
			"require": {
				"default": "./dist/rpc-client/index.js",
				"types": "./dist/rpc-client/index.d.ts"
			}
		}
	},
	"files": [
		"dist",
		"README.md",
		"CHANGELOG.md"
	],
	"main": "dist/index.js",
	"name": "@jsandy/rpc",
	"peerDependencies": {
		"hono": "^4.7.11",
		"zod": "^4.0.5"
	},
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"directory": "packages/rpc",
		"type": "git",
		"url": "git+https://github.com/JacobDevelops/jsandy.git"
	},
	"scripts": {
		"build": "bun build.ts && tsc --emitDeclarationOnly",
		"clean": "rm -rf dist",
		"prebuild": "bun run clean",
		"test": "bun test",
		"test:coverage": "bun test --coverage",
		"test:watch": "bun test --watch",
		"typecheck": "tsc --noEmit"
	},
	"types": "dist/index.d.ts",
	"version": "2.1.2"
