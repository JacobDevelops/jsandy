{
	"name": "@jsandy/rpc",
	"version": "1.0.0",
	"publishConfig": {
		"access": "public"
	},
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"files": [
		"dist",
		"README.md",
		"CHANGELOG.md"
	],
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.mts",
				"default": "./dist/index.mjs"
			},
			"require": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			}
		}
	},
	"scripts": {
		"build": "bun build.ts && tsc --emitDeclarationOnly",
		"check-types": "tsc --noEmit",
		"prebuild": "rm -rf dist",
		"test": "bun test",
		"test:watch": "bun test --watch",
		"test:coverage": "bun test --coverage"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "^4.20241218.0",
		"@jsandy/builder": "1.0.0",
		"@jsandy/typescript-config": "workspace:*",
		"@types/bun": "^1.2.15",
		"@types/node": "^22.15.29",
		"typescript": "^5.2.2"
	},
	"peerDependencies": {
		"hono": "^4",
		"zod": "^3.25 || ^4"
	},
	"dependencies": {
		"superjson": "^2.2.2"
	}
}
