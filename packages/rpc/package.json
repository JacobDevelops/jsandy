{
	"dependencies": {
		"superjson": "^2.2.2"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "^4.20250601.0",
		"@jsandy/builder": "1.0.2",
		"@jsandy/typescript-config": "workspace:*",
		"@types/bun": "^1.2.15",
		"@types/node": "^22.15.29",
		"@types/react": "^19.2.7",
		"typescript": "^5.8.3"
	},
	"exports": {
		".": {
			"import": {
				"default": "./dist/index.mjs",
