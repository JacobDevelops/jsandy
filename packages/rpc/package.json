{
	"name": "@jsandy/rpc",
	"version": "1.2.1",
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/JacobDevelops/jsandy.git",
		"directory": "packages/rpc"
	},
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"files": ["dist", "README.md", "CHANGELOG.md"],
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.mts",
				"default": "./dist/index.mjs"
			},
			"require": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			}
		}
	},
	"scripts": {
		"build": "bun build.ts && tsc --emitDeclarationOnly",
		"check-types": "tsc --noEmit",
		"clean": "rm -rf dist",
		"prebuild": "bun run clean",
		"test": "bun test",
		"test:watch": "bun test --watch",
		"test:coverage": "bun test --coverage"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "^4.20250601.0",
		"@jsandy/builder": "1.0.1",
		"@jsandy/typescript-config": "workspace:*",
		"@types/bun": "^1.2.15",
		"@types/node": "^22.15.29",
		"typescript": "^5.8.3"
	},
	"peerDependencies": {
		"hono": "^4.7.11",
		"zod": "^4.0.5"
	},
	"dependencies": {
		"superjson": "^2.2.2"
	}
}
