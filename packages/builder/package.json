{
	"name": "@jsandy/builder",
	"version": "0.0.0",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"publishConfig": {
		"access": "public"
	},
	".": {
		"import": {
			"types": "./dist/index.d.mts",
			"default": "./dist/index.mjs"
		},
		"require": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.js"
		}
	},
	"files": ["dist", "README.md", "CHANGELOG.md"],
	"scripts": {
		"build": "bun build.ts",
		"prebuild": "rm -rf ./dist"
	},
	"devDependencies": {
		"@jsandy/typescript-config": "workspace:*",
		"@types/node": "^22.10.10",
		"typescript": "^5.8.2"
	},
	"dependencies": {
		"esbuild": "^0.25.0",
		"tslib": "^2.7.0",
		"tsup": "^8.0.2"
	}
}
