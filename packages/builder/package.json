{
	"name": "@jsandy/builder",
	"version": "1.0.2",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"publishConfig": {
		"access": "public"
	},
	".": {
		"import": {
			"types": "./dist/index.d.mts",
			"default": "./dist/index.mjs"
		},
		"require": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.js"
		}
	},
	"files": [
		"dist",
		"README.md",
		"CHANGELOG.md"
	],
	"scripts": {
		"build": "bun build.ts",
		"clean": "rm -rf dist",
		"prebuild": "bun run clean"
	},
	"devDependencies": {
		"@jsandy/typescript-config": "workspace:*",
		"@types/node": "^22.15.29",
		"typescript": "^5.8.3"
	},
	"dependencies": {
		"esbuild": "^0.25.5",
		"tslib": "^2.8.1",
		"tsup": "^8.5.0"
	}
}
