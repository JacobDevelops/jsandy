		"fumadocs-mdx": "^13.0.2",
		"fumadocs-ui": "^16.0.5",
		"lucide-react": "^0.548.0",
		"next": "16.0.0",
		"react": "^19.2.0",
		"react-dom": "^19.2.0",
		"tailwind-merge": "^3.3.1"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.1",
		"@opennextjs/cloudflare": "^1.11.0",
		"@tailwindcss/postcss": "^4.1.16",
		"@types/mdx": "^2.0.13",
		"@types/node": "^24.9.1",
		"@types/react": "^19.2.2",
		"@types/react-dom": "^19.2.2",
		"postcss": "^8.5.6",
		"tailwindcss": "^4.1.16",
